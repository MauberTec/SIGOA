    function Deletar(id) {
        swal({
            title: "Excluir. Tem certeza?",
            icon: "warning",
            buttons: [
                'Não',
                'Sim'
            ],
            dangerMode: true,
            focusCancel: true
        }).then(function (isConfirm) {
            if (isConfirm) {
                var response = POST("/Orcamento/Orcamento_Excluir", JSON.stringify({ id: id }))
                if (response.erroId >= 1) {
                    swal({
                        type: 'success',
                        title: 'Sucesso',
                        text: 'Registro excluído com sucesso'
                    });

                    $('#tblOrcamentos').DataTable().ajax.reload();
                }
                else {
                    swal({
                        type: 'error',
                        title: 'Aviso',
                        text: 'Erro ao excluir registro'
                    });
                }
                return true;
            } else {
                return false;
            }
        })
        return false;
    }

    function AtivarDesativarOrcamento(id, ativar) {

        if (id >= 0) {
            var form = this;
            swal({
                title: (ativar == 1 ? "Ativar" : "Desativar") + ". Tem certeza?",
                icon: "warning",
                buttons: [
                    'Não',
                    'Sim'
                ],
                dangerMode: true,
                focusCancel: true
            }).then(function (isConfirm) {
                if (isConfirm) {

                    var url = "/Orcamento/Orcamento_AtivarDesativar";
                    var response = POST(url, JSON.stringify({ id: id }))
                    if (response.erroId == 1) {
                        swal({
