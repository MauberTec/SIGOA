<div style="width:100%;" id="divFiltros">
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">Localizar</h3>
        </div>
        <div class="box-body">
            <div class="form-group">
                <div class="col-sm-4">
                    <label class="control-label">Rodovia</label>
                    <input type="text" class="form-control" id="txtFiltroRodovia" style="display:none;" maxlength="150" onkeydown="if (event.keyCode == 13) { ExecutarFiltroOrcamento(); }">

                    <table style="width:100%">
                        <tr>
                            <td style="width:100%">
                                <input id="txtFiltroRodovias" name="txtFiltroRodovias" class="form-control" onclick="return abrirLocalizarRodovias();"
                                       placeholder="Rodovia" readonly maxlength="40" style="margin-left: 5px !important;"
                                       autocomplete="off" />
                            </td>
                            <td>
                                <button style="display:inline; width:40px; height:25px; margin-left:10px; border: solid 1px #ccc;" onclick="return abrirLocalizarRodovias();">...</button>
                            </td>
                        </tr>
                    </table>

                </div>

                <div class="col-sm-4">
                    <label class="control-label">Objetos</label>
                    <input type="text" class="form-control" id="txtFiltroObjetos" style="display:none;" maxlength="255" onkeydown="if (event.keyCode == 13) { ExecutarFiltroOrcamento(); }">

                    <table style="width:100%">
                        <tr>
                            <td style="width:100%">
                                <input id="txtFiltroObj_codigo" name="txtFiltroObj_codigo" class="form-control"
                                       placeholder="Objeto" readonly maxlength="40" onclick="return abrirLocalizarObjetos();"
                                       autocomplete="off" style="margin-left: 5px !important;" />
                            </td>
                            <td>
                                <button style="display:inline; width:40px; height:25px; margin-left:10px; border: solid 1px #ccc;" onclick="return abrirLocalizarObjetos();">...</button>
                            </td>
                        </tr>
                    </table>

                </div>

                <div class="col-sm-4 ">
                    <label class="control-label">Status</label>
                    @Html.DropDownList("cmbFiltroStatusOrcamento", null, "-- Selecione --", new { @class = "form-control " })
                </div>

                <div>
                    <div class="btn-box-tool pull-right" style="margin-top:10px">
                        <button type="reset" class="btn btn-info" onclick="LimparFiltroOrcamento();"><i class="fa fa-eraser margin-r-5"></i> Limpar</button>
                        <button type="submit" class="btn btn-primary" onclick="ExecutarFiltroOrcamento();" style="margin-right: 10px !important;"><i class="fa fa-search margin-r-5"></i> Pesquisar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

